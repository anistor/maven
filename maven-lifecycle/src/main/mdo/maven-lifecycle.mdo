<model>
  <id>build-lifecycle</id>
  <name>BuildLifecycle</name>
  <description>Model for lifecycle specifications starting in Maven 2.1</description>
  <defaults>
    <default>
      <key>package</key>
      <value>org.apache.maven.model</value>
    </default>
  </defaults>
  <classes>
    <class rootElement="true">
      <name>Lifecycles</name>
      <version>1.0.0</version>
      <description>Specifies phase bindings for clean, site, and default lifecycles.</description>
      <fields>
        <field>
          <version>1.0.0</version>
          <name>packaging</name>
          <type>String</type>
          <required>true</required>
          <description>POM packaging to which this lifecycle specification applies.</description>
        </field>
        <field xml.tagName="clean">
          <version>1.0.0</version>
          <name>cleanLifecycle</name>
          <required>false</required>
          <description>The bindings for the clean lifecycle</description>
          <association>
            <type>CleanLifecycle</type>
          </association>
        </field>
        <field xml.tagName="build">
          <version>1.0.0</version>
          <name>buildLifecycle</name>
          <required>true</required>
          <description>The bindings for the main build (default) lifecycle</description>
          <association>
            <type>BuildLifecycle</type>
          </association>
        </field>
        <field xml.tagName="site">
          <version>1.0.0</version>
          <name>siteLifecycle</name>
          <required>false</required>
          <description>The bindings for the site lifecycle</description>
          <association>
            <type>SiteLifecycle</type>
          </association>
        </field>
      </fields>
    </class>
    <class>
      <name>CleanLifecycle</name>
      <version>1.0.0</version>
      <fields>
        <field xml.tagName="pre-clean">
          <name>preClean</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field xml.tagName="clean">
          <name>clean</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field xml.tagName="post-clean">
          <name>postClean</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
      </fields>
    </class>
    <class>
      <name>BuildLifecycle</name>
      <version>1.0.0</version>
      <fields>
        <field>
          <name>validate</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field>
          <name>initialize</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field xml.tagName="generate-sources">
          <name>generateSources</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field xml.tagName="process-sources">
          <name>processSources</name>
          <version>1.0.0</version>
          <association>
            <type>Phase</type>
          </association>
        </field>
        <field xml.tagName="generate-resources">
          <name>generateResources</name>
